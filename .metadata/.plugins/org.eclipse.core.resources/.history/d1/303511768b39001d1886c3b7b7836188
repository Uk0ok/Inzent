FILEPATH2=C:\\dev\\sam\\testSql2.sam
UNIQUEFILEPATH=D:\\dev\\uniquetest.txt

QUERY1=SELECT ELEMENTID, DESCR, USERSCLASS, ECLASSID FROM ASYSELEMENT WHERE ELEMENTID LIKE '2%'
QUERY2=SELECT ELEMENTID, VOLUMEID, FILEKEY, FILESIZE, CREATEDATE, LASTACCESS, CONTENTTYPE, CONTENTCLASSID, CHECKHINT, WRITEDATE, RETENTION FROM ASYSCONTENTELEMENT WHERE ELEMENTID LIKE '2%'
QUERY3=INSERT INTO ASYSELEMENT (ELEMENTID, DESCR, USERSCLASS, ECLASSID) VALUES (?, ?, ?, ?)
QUERY4=INSERT INTO ASYSCONTENTELEMENT(ELEMENTID, VOLUMEID, FILEKEY, FILESIZE, CREATEDATE, LASTACCESS, CONTENTTYPE, CONTENTCLASSID, CHECKHINT, WRITEDATE, RETENTION) VALUES (?, ?, ?, TO_NUMBER(?), TO_DATE(?, 'YYYY-MM-DD HH24:MI:SS'), TO_DATE(?, 'YYYY-MM-DD HH24:MI:SS'), TO_NUMBER(?), ?, TO_NUMBER(?), TO_DATE(?, 'YYYY-MM-DD HH24:MI:SS'), TO_NUMBER(?))

